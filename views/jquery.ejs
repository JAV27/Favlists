<script>
    $(document).ready(function() {
        
        $('.results .result').on('click', function() {
            console.log($(this).text());
            $('.selected').removeClass('selected');
            $(this).addClass('selected');

            $('input[type="hidden"]').val($('.selected').attr('id'));

        });

        $('.navbar i.fa-bars').on('click', function() {
            $('.extended-navbar').css('display', 'flex');
            $(this).hide();
            $('.navbar i.fa-times').show();
        });

        $('.navbar i.fa-times').on('click', function() {
            $('.extended-navbar').hide();
            $(this).hide();
            $('.navbar i.fa-bars').show();
        });


    });

</script>